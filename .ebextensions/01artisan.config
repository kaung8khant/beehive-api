container_commands:
    01-optimize-autoloader:
        command: "/usr/bin/composer.phar dump-autoload --optimize"

    02-optimize-config-loading:
        command: "php artisan config:cache"
        cwd: "/var/www/html"

    03-optimize-route-loading:
        command: "php artisan route:cache"
        cwd: "/var/www/html"

